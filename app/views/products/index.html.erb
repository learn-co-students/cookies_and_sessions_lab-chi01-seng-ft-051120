<h1> Add Item to Cart </h1>
<% if cart != [] %>
<ul>
    <% cart.each do |item| %>
        <li> <%= item %> </li>
    <% end %> 
</ul>
<% end %>
<%= form_tag("/add") do %>
    <%= label_tag('product', "Product to Add") %>
    <%= text_field_tag('product') %>
    <%= submit_tag "add to cart" %>
<% end %>